{
  "name": "shops-vendure-backend",
  "version": "0.42.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/index",
    "debug:test": "yarn build && export LOCAL_ENV=.env.test && node --inspect ./dist/index",
    "serve": "nodemon -e ts --watch '../packages/' --watch './src/' --exec \"yarn build && node ./dist/index\"",
    "serve:prod": "export LOCAL_ENV=.env && yarn serve",
    "serve:test": "export LOCAL_ENV=.env.test && yarn serve",
    "script:prod": "export LOCAL_ENV=.env && yarn ts-node",
    "script:test": "export LOCAL_ENV=.env.test && yarn ts-node",
    "copy": "yarn copyfiles -u 1 'src/__admin-ui/dist/**/*' dist",
    "build": "yarn tsc && yarn copy",
    "build:admin": "yarn ts-node src/compile-admin-ui.ts",
    "migration:generate:test": "export LOCAL_ENV=.env.test && yarn ts-node migration generate",
    "migration:run:test": "export LOCAL_ENV=.env.test && yarn ts-node migration run",
    "migration:revert:test": "export LOCAL_ENV=.env.test && yarn ts-node migration revert",
    "migration:generate:prod": "export LOCAL_ENV=.env && yarn ts-node migration generate",
    "migration:run:prod": "export LOCAL_ENV=.env && yarn ts-node migration run",
    "migration:revert:prod": "export LOCAL_ENV=.env && yarn ts-node migration revert"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "^4.1.2",
    "@mollie/api-client": "3.6.0",
    "@vendure/admin-ui-plugin": "1.4.3",
    "@vendure/asset-server-plugin": "1.4.3",
    "@vendure/core": "1.4.3",
    "@vendure/email-plugin": "1.4.3",
    "@vendure/payments-plugin": "1.4.3",
    "@vendure/ui-devkit": "1.4.3",
    "copyfiles": "^2.4.1",
    "mysql": "^2.18.1",
    "typescript": "4.3.5",
    "vendure-plugin-dutch-postalcode": "2.2.2",
    "vendure-plugin-goedgepickt": "2.2.2",
    "vendure-plugin-google-cloud-tasks": "2.2.2",
    "vendure-plugin-google-storage-assets": "2.2.2",
    "vendure-plugin-invoices": "^2.2.2",
    "vendure-plugin-myparcel": "2.2.2",
    "vendure-plugin-webhook": "2.2.2",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@types/papaparse": "^5.3.2",
    "@types/prompt": "^1.1.2",
    "localtunnel": "^2.0.2",
    "nodemon": "^2.0.13",
    "papaparse": "^5.3.1",
    "prompt": "^1.2.1",
    "ts-node": "^10.5.0"
  }
}
