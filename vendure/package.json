{
  "name": "shops-vendure-backend",
  "version": "0.60.1",
  "private": true,
  "scripts": {
    "start": "node ./dist/index",
    "start:dev": "ts-node test/start-dev-server.ts",
    "debug:test": "yarn build && export LOCAL_ENV=.env.test && node --inspect ./dist/index",
    "serve": "nodemon -e ts --watch './src/' --exec \"yarn build && node ./dist/index\"",
    "serve:prod": "export LOCAL_ENV=.env && yarn serve",
    "serve:test": "export LOCAL_ENV=.env.test && yarn serve",
    "serve:wkw-test": "export LOCAL_ENV=.env.wkw-test && yarn serve",
    "serve:wkw-prod": "export LOCAL_ENV=.env.wkw-prod && yarn serve",
    "script:prod": "export LOCAL_ENV=.env && yarn ts-node",
    "script:test": "export LOCAL_ENV=.env.test && yarn ts-node",
    "copy": "yarn copyfiles -u 1 'src/__admin-ui/dist/**/*' dist",
    "build": "yarn tsc && yarn copy",
    "build:admin": "yarn ts-node src/compile-admin-ui.ts",
    "generate-migration": "yarn ts-node migration generate",
    "generate-migration:test": "export LOCAL_ENV=.env.test && yarn generate-migration",
    "generate-migration:prod": "export LOCAL_ENV=.env && yarn generate-migration",
    "generate-migration:wkw-test": "export LOCAL_ENV=.env.wkw-test && yarn generate-migration",
    "generate-migration:wkw-prod": "export LOCAL_ENV=.env.wkw-prod && yarn generate-migration",
    "run-migration": "yarn ts-node migration run",
    "run-migration:test": "export LOCAL_ENV=.env.test && yarn run-migration",
    "run-migration:prod": "export LOCAL_ENV=.env && yarn run-migration",
    "run-migration:wkw-test": "export LOCAL_ENV=.env.wkw-test && yarn run-migration",
    "run-migration:wkw-prod": "export LOCAL_ENV=.env.wkw-prod && yarn run-migration",
    "revert-migration": "yarn ts-node migration revert",
    "rever-migration:test": "export LOCAL_ENV=.env.test && yarn revert-migration",
    "revert-migration:prod": "export LOCAL_ENV=.env && yarn revert-migration",
    "revert-migration:wkw-test": "export LOCAL_ENV=.env.wkw-test && yarn revert-migration",
    "revert-migration:wkw-prod": "export LOCAL_ENV=.env.wkw-prod && yarn revert-migration"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "5.1.5",
    "@google-cloud/storage": "^6.5.2",
    "@mollie/api-client": "3.6.0",
    "@vendure/admin-ui-plugin": "1.7.3",
    "@vendure/asset-server-plugin": "1.7.3",
    "@vendure/core": "1.7.3",
    "@vendure/email-plugin": "1.7.3",
    "@vendure/payments-plugin": "1.7.3",
    "@vendure/ui-devkit": "1.7.3",
    "copyfiles": "^2.4.1",
    "mysql": "^2.18.1",
    "tmp": "^0.2.1",
    "typescript": "4.3.5",
    "vendure-plugin-admin-ui-helpers": "4.0.1",
    "vendure-plugin-coinbase": "4.0.1",
    "vendure-plugin-dutch-postalcode": "4.0.1",
    "vendure-plugin-e-boekhouden": "4.0.1",
    "vendure-plugin-goedgepickt": "4.0.1",
    "vendure-plugin-google-cloud-tasks": "4.0.1",
    "vendure-plugin-google-storage-assets": "4.0.1",
    "vendure-plugin-invoices": "4.0.3",
    "vendure-plugin-myparcel": "4.0.1",
    "vendure-plugin-order-export": "4.0.1",
    "vendure-plugin-sendcloud": "^4.0.1",
    "vendure-plugin-shipping-by-weight-and-country": "4.0.1",
    "vendure-plugin-stock-monitoring": "^4.0.1",
    "vendure-plugin-webhook": "4.0.1",
    "winston": "3.8.2"
  },
  "devDependencies": {
    "@types/papaparse": "^5.3.5",
    "@types/prompt": "^1.1.3",
    "@types/tmp": "^0.2.3",
    "@vendure/testing": "^1.7.3",
    "localtunnel": "^2.0.2",
    "nodemon": "^2.0.20",
    "papaparse": "^5.3.2",
    "prompt": "^1.3.0",
    "ts-node": "^10.8.1"
  }
}
