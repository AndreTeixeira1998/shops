{
  "name": "shops",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "run:server": "npx ts-node ./src/index.ts",
    "run:worker": "npx ts-node ./src/index-worker.ts",
    "start": "npm run run:server",
    "serve": "nodemon -e ts  --exec \"npx ts-node ./src/index-dev.ts\"",
    "serve:prod": "export SHOP_ENV=.env && npm run serve",
    "serve:test": "export SHOP_ENV=.env.test && npm run serve",
    "build": "npx tsc",
    "migration:generate": "npx ts-node migration generate",
    "migration:run": "npx ts-node migration run",
    "migration:revert": "npx ts-node migration revert"
  },
  "resolutions": {
    "tslib": "1.11.2"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.3.0",
    "@mollie/api-client": "^3.2.4",
    "@vendure/admin-ui-plugin": "0.17.1",
    "@vendure/asset-server-plugin": "0.17.1",
    "@vendure/core": "0.17.1",
    "@vendure/email-plugin": "0.17.1",
    "mysql": "^2.18.1",
    "tmp": "^0.2.1",
    "typescript": "4.0.3",
    "ts-node": "^9.0.0",
    "@types/tmp": "^0.2.0"
  },
  "devDependencies": {
    "@types/localtunnel": "^2.0.0",
    "localtunnel": "^2.0.0",
    "nodemon": "^2.0.4"
  }
}
