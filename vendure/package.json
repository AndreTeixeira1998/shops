{
  "name": "shops-vendure-backend",
  "version": "1.4.2",
  "private": true,
  "scripts": {
    "start": "node ./dist/index",
    "start:dev": "ts-node test/start-dev-server.ts",
    "debug:test": "yarn build && export ENV_FILE=.env.test && node --inspect ./dist/index",
    "serve": "nodemon -e ts --watch './src/' --exec \"yarn build && node ./dist/index\"",
    "serve:prod": "export ENV_FILE=.env && yarn serve",
    "serve:test": "export ENV_FILE=.env.test && yarn serve",
    "serve:wkw-test": "export ENV_FILE=.env.wkw-test && yarn serve",
    "serve:wkw-prod": "export ENV_FILE=.env.wkw-prod && yarn serve",
    "script:prod": "export ENV_FILE=.env && yarn ts-node",
    "script:test": "export ENV_FILE=.env.test && yarn ts-node",
    "script:wkw-test": "export ENV_FILE=.env.wkw-test && yarn ts-node",
    "script:wkw-prod": "export ENV_FILE=.env.wkw-prod && yarn ts-node",
    "copy": "yarn copyfiles -u 1 'src/__admin-ui/dist/**/*' dist",
    "build": "yarn tsc && yarn copy",
    "build:admin": "rimraf src/__admin-ui && yarn ts-node src/compile-admin-ui.ts",
    "generate-migration": "yarn ts-node migration generate",
    "generate-migration:test": "export ENV_FILE=.env.test && yarn generate-migration",
    "generate-migration:prod": "export ENV_FILE=.env && yarn generate-migration",
    "generate-migration:wkw-test": "export ENV_FILE=.env.wkw-test && yarn generate-migration",
    "generate-migration:wkw-prod": "export ENV_FILE=.env.wkw-prod && yarn generate-migration",
    "run-migration": "yarn ts-node migration run",
    "run-migration:test": "export ENV_FILE=.env.test && yarn run-migration",
    "run-migration:prod": "export ENV_FILE=.env && yarn run-migration",
    "run-migration:wkw-test": "export ENV_FILE=.env.wkw-test && yarn run-migration",
    "run-migration:wkw-prod": "export ENV_FILE=.env.wkw-prod && yarn run-migration",
    "revert-migration": "yarn ts-node migration revert",
    "rever-migration:test": "export ENV_FILE=.env.test && yarn revert-migration",
    "revert-migration:prod": "export ENV_FILE=.env && yarn revert-migration",
    "revert-migration:wkw-test": "export ENV_FILE=.env.wkw-test && yarn revert-migration",
    "revert-migration:wkw-prod": "export ENV_FILE=.env.wkw-prod && yarn revert-migration"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "6.0.0",
    "@google-cloud/storage": "7.7.0",
    "@mollie/api-client": "3.7.0",
    "@pinelab/vendure-plugin-admin-ui-helpers": "1.1.0",
    "@pinelab/vendure-plugin-coinbase": "1.1.0",
    "@pinelab/vendure-plugin-dutch-postalcode": "1.1.0",
    "@pinelab/vendure-plugin-e-boekhouden": "1.1.0",
    "@pinelab/vendure-plugin-goedgepickt": "1.1.5",
    "@pinelab/vendure-plugin-google-cloud-tasks": "1.5.0",
    "@pinelab/vendure-plugin-google-storage-assets": "1.1.0",
    "@pinelab/vendure-plugin-invoices": "1.2.0",
    "@pinelab/vendure-plugin-limit-variant-per-order": "1.1.0",
    "@pinelab/vendure-plugin-metrics": "1.2.0",
    "@pinelab/vendure-plugin-myparcel": "1.2.0",
    "@pinelab/vendure-plugin-order-export": "1.1.0",
    "@pinelab/vendure-plugin-popularity-scores": "^1.6.1",
    "@pinelab/vendure-plugin-selectable-gifts": "1.1.1",
    "@pinelab/vendure-plugin-sendcloud": "1.2.3",
    "@pinelab/vendure-plugin-shipping-by-weight-and-country": "1.1.0",
    "@pinelab/vendure-plugin-stock-monitoring": "1.3.2",
    "@pinelab/vendure-plugin-variant-bulk-update": "1.1.0",
    "@pinelab/vendure-plugin-webhook": "1.1.1",
    "@types/adm-zip": "0.5.0",
    "@vendure/admin-ui-plugin": "2.1.5",
    "@vendure/asset-server-plugin": "2.1.5",
    "@vendure/core": "2.1.5",
    "@vendure/email-plugin": "2.1.5",
    "@vendure/migrate-v2": "^0.4.0",
    "@vendure/payments-plugin": "2.1.5",
    "@vendure/ui-devkit": "2.1.5",
    "adm-zip": "0.5.10",
    "copyfiles": "2.4.1",
    "mysql": "2.18.1",
    "tmp": "0.2.1",
    "typescript": "4.9.5",
    "winston": "3.11.0"
  },
  "devDependencies": {
    "@types/papaparse": "5.3.14",
    "@types/prompt": "1.1.8",
    "@types/tmp": "0.2.3",
    "@vendure/testing": "2.1.5",
    "localtunnel": "2.0.2",
    "nodemon": "2.0.20",
    "papaparse": "5.4.1",
    "prompt": "1.3.0",
    "ts-node": "10.9.2"
  }
}
