{
  "name": "shops",
  "version": "0.18.1",
  "private": true,
  "scripts": {
    "run:server": "yarn ts-node ./src/index.ts",
    "run:worker": "yarn ts-node ./src/index-worker.ts",
    "start": "node ./dist/index",
    "serve": "nodemon -e ts  --ignore './src/__admin-ui/' --exec \"yarn ts-node ./src/index-dev.ts\"",
    "serve:prod": "export SHOP_ENV=.env && yarn run serve",
    "serve:test": "export SHOP_ENV=.env.test && yarn run serve",
    "copy": "yarn copyfiles -u 1 src/__admin-ui/dist/** dist",
    "build": "yarn tsc && yarn copy",
    "migration:generate": "yarn ts-node migration generate",
    "migration:run": "yarn ts-node migration run",
    "migration:revert": "yarn ts-node migration revert",
    "test": "export SHOP_ENV=.env.test && ",
    "prod": "export SHOP_ENV=.env && "
  },
  "resolutions": {
    "tslib": "1.11.2"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.3.0",
    "@mollie/api-client": "^3.2.4",
    "@types/tmp": "^0.2.0",
    "@vendure/admin-ui-plugin": "0.18.1",
    "@vendure/asset-server-plugin": "0.18.1",
    "@vendure/core": "0.18.1",
    "@vendure/email-plugin": "0.18.1",
    "@vendure/ui-devkit": "^0.18.1",
    "copyfiles": "^2.4.1",
    "mysql": "^2.18.1",
    "tmp": "^0.2.1",
    "ts-node": "^9.0.0",
    "typescript": "4.0.3"
  },
  "devDependencies": {
    "@types/localtunnel": "^2.0.0",
    "localtunnel": "^2.0.0",
    "nodemon": "^2.0.4"
  }
}
