{
  "module": "commonjs",
  "allowSyntheticDefaultImports": true,
  "esModuleInterop": true,
  "emitDecoratorMetadata": true,
  "experimentalDecorators": true,
  "target": "es2017",
  "strict": true,
  "sourceMap": false,
  "skipLibCheck": true,
  "compilerOptions": {
    "outDir": "./dist",
    "baseUrl": "./src/"
  },
  "include": ["src"],
  "exclude": ["node_modules", "migration", "script", "__admin-ui"]
}
