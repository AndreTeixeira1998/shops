  },
  "resolutions": {
    "graphql": "^14.7.0"
  },
  "dependencies": {
    "buefy": "^0.9.5",
    "pinelab-storefront-client": "0.21.0"
  },
  "devDependencies": {
    "@gridsome/source-graphql": "^0.1.0",
    "concurrently": "^5.3.0",
    "gridsome": "^0.7.23",
    "http-server": "^0.12.3",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.7",
    "sass-loader": "^10.1.1",
    "webpack-bundle-analyzer": "^4.3.0"
  }
}
