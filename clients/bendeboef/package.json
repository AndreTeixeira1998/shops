    "outfront": "^1.1.1",
    "pinelab-storefront": "0.66.4",
    "vue-gtag": "^1.16.1"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "graphql-tag": "^2.12.5",
    "gridsome": "^0.7.23",
    "http-server": "^0.12.3",
    "nodemon": "^2.0.7",
    "sass": "1.32.13",
    "sass-loader": "^10.1.1",
    "webpack-bundle-analyzer": "^4.3.0"
